<!DOCTYPE html><html>    <head>		<meta http-equiv="content-type" content="text/html; charset=UTF-8"/>        <title>{{ "databasecfg.title"|trans }}</title>		<link rel="stylesheet" href="{{ url('css/bootstrap.min.css') }}">		<script src="{{ url('js/jquery-1.9.1.js')}}"></script>		<script src="{{ url('js/bootstrap.min.js')}}"></script>    </head>    <body>       		<div class="container-fluid"> 			<div id="body" class=" col-xs-offset-3 col-xs-6 col-sm-offset-3 col-sm-6">				<div class="panel panel-default">					<div class="panel-heading">{{ "databasecfg.title"|trans }}</div>					<div class="panel-body">						<form role="form" action="{{ url('config/database') }}" method="post">						<div class="form-group">							<label for="server">{{ "databasecfg.server"|trans }} :</label>							<input type="text" class="form-control" name="server" id="server" value="{{database['default']['server']}}">						</div>						<div class="form-group">							<label for="port">{{ "databasecfg.port"|trans }} :</label>							<input type="text" class="form-control" name="port" id="port" value="{{database['default']['port']}}">						</div>						<div class="form-group">							<label for="database">{{ "databasecfg.database"|trans }} :</label>							<input type="text" class="form-control" name="database" id="database" value="{{database['default']['database']}}">						</div>						<div class="form-group">							<label for="user">{{ "databasecfg.user"|trans }} :</label>							<input type="text" class="form-control" name="user" id="user" value="{{database['default']['user']}}">						</div>						<div class="form-group">							<label for="password">{{ "databasecfg.password"|trans }} :</label>							<input type="password" class="form-control" name="password" id="password" value="{{database['default']['password']}}">						</div>						<input type="submit" class="btn btn-primary" value="{{ "global.save"|trans }}">						</form>					</div>				</div>			</div>		</div>    </body></html>